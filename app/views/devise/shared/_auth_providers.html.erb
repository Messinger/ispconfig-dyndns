
<%- if devise_mapping.omniauthable? %>
<div class="col-md-12 col-xs-18">
  <div id="authproviders" class="buttonlist">
    <ul class="list-group">
    <%- resource_class.omniauth_providers.each do |provider| %>
      <li class="list-group-item"><%= link_to content_tag("i",'',:class => "fa fa-#{auth_provider_to_cssname(provider)}"), omniauth_authorize_path(resource_name, provider), :class => "btn btn-block btn-social-icon btn-#{auth_provider_to_cssname(provider)}" %>
      </li>
    <% end -%>
    </ul>
  </div>
</div>
<% end -%>
